{
    "vsicons.presets.angular": false,
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "outDir": "build",
        "noImplicitAny": true,
        "baseUrl": ".",
        "allowJs": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "build",
        "coverage",
        "docs",
        "node_modules"
    ]
}